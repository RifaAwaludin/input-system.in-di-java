def is_kabisat(year) :
        # Algoritma :
        if year %400==0 :
        print(‘true’)
        elif year %4!=0 :
        print(‘false’)
        elif year %400!=0 :
        if year %100!=0 :
        if year %4==0 :
        print(‘true’)
        elif year %100==0 :
        print(‘false’)